<Project Sdk="Godot.NET.Sdk/4.3.0">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net7.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'ios' ">net8.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="scenes\" />
    <Folder Include="Scripts\UI\" />
    <Folder Include="Scripts\Utility\" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Scripts\CubeSphereGenerator.cs.bak" />
    <Content Include="Scripts\Shaders\SphereTexture.gdshader" />
    <Content Include="Scripts\Shaders\WorldShader.gdshader" />
    <Content Include="Scripts\Shaders\WorldShader.gdshader.uid" />
  </ItemGroup>
</Project>